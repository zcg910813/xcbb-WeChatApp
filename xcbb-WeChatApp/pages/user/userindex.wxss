/* pages/user/userindex.wxss */
.userwrap{
  padding: 38rpx 0;
  background: rgb(39, 31, 56);
  text-align: center
}
.img_border image{
  border-radius: 50%;
  border: 1rpx solid #fff;
  width: 100rpx;
  margin: 0;
  padding: 0;
  height: 100rpx;
  display: block
}
.img_border{
  padding: 4rpx;
  border: 2rpx solid #fff;
  display:inline-block;
  margin: 0 auto;
  border-radius: 50%;
  position: relative;
}
.local_tyrants{
  background: url('data:image/png;base64,') no-repeat;
  width: 63rpx;
  height: 36rpx;
  background-size: 100% 100%;
  position: absolute;
  bottom: -10rpx;
  font-size: 16rpx;
  color: #178a7b;
  text-indent: 25rpx;
  line-height: 36rpx;
  left: 20rpx;
}

.share{
  width: 34rpx !important;
  height: 34rpx !important;
  position: absolute;
  right: -50rpx;
  border-radius: 0 !important;
  border: none !important;
  bottom: -10rpx;
}

.nameone{
  color: #f9c54f;
}
.nametow{
  color: #fff;
}


.headimg_name{
  position: relative;
  width: 468rpx;
  margin: 0 auto
}
.headimg_name text{
  font-size: 27rpx;
  font-family: "微软雅黑"
}

.moon{
  position: absolute;
  right: 0;
  background: #bdb5f0;
  padding: 2rpx 4rpx;
  top: 8rpx;
  color: #f6f1de;
  border-radius: 10rpx;
}
.moon text{
  font-size: 16rpx;
  float: left;
  margin-top: 5rpx;
}
.moon image{
  width: 26rpx;
  height: 22rpx;
  float: left;
  margin-top: 4rpx;
}

.progress{
  width: 290rpx;
  line-height: 19rpx;
  height: 19rpx;
  background: #231b30;
  margin: 0 auto;
  border-radius: 10rpx;
  margin-top: 20rpx;
  border:1px solid #000;
}


.gifss{
  position: fixed;
  top: 5rpx;
  left: 0rpx;
}
.progress{
  position: relative;
}
.progressin{
  background: url('data:image/png;base64,');
  height: 100%;
  border-radius: 10rpx;
  position: absolute;
  left: 0;
  z-index: 0;
}
.progress text{
  font-size: 18rpx;
  color: #fff;
  text-align: center;
  z-index: 5;
  position: relative;
  padding: 0 !important;
  line-height: 19rpx !important;
  display: block;
  text-shadow:#000 1px 0 0,#000 0 1px 0,#000 -1px 0 0,#000 0 -1px 0;
  -webkit-text-shadow:#000 1px 0 0,#000 0 1px 0,#000 -1px 0 0,#000 0 -1px 0;
  -moz-text-shadow:#000 1px 0 0,#000 0 1px 0,#000 -1px 0 0,#000 0 -1px 0;
}
.tuhao{
  width: 38rpx;
  height: 38rpx;
  position: absolute;
  left: -19rpx;
  top:-8rpx;
  z-index: 2;
}
.explain{
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  right: -40rpx;
  top: -5rpx;
}



.usernames{
  color: #b7b7ff;
  display: block;
  font-size: 27rpx;
  padding-top: 20rpx
}
.clear{
  clear: both;
}
.usersignature{
  color: #baae72;
  font-size: 27rpx;
  margin-top: 18rpx;
}


.user_statistics{
  margin-top: 23rpx;
}
.user_statistics .user_statistics_li{
  width: 50%;
  float: left;
}
.user_statistics .user_statistics_li text{
  display: block;
  color: #baae72;
  font-size: 26rpx;
}
.user_statistics .user_statistics_li .num{
  color: #b8b5ff;
  font-size: 28rpx;
  font-family: "微软雅黑"
}
.user_statistics .user_statistics_li:nth-child(1){
  border-right: 1px solid #3e3b50;
  box-sizing: border-box;
}
.userbkbj{
  height: 50rpx;
  width: 100%;
  float: left;
}



.list_ul .list_ul_li{
  padding: 24rpx 22rpx;
  background: #271f36;
  line-height: 35rpx;
  border-bottom: 1px solid #3e364b
}
.list_ul .list_ul_li .list_ul_li_text{
  float: left;
}
.list_ul_li_right{
  float: right
}
.list_ul_li_right .goldimg{
  width: 35rpx;
  height: 35rpx;
}
.righttriangle{
  width: 17rpx;
  height: 30rpx;
}
.list_ul_li_text{
  color: #fff;
  font-family: "微软雅黑";
  font-size: 26rpx;
}
.list_ul_li_right text{
  font-family: "微软雅黑";
  color: #bcaa7a;
  font-size: 26rpx;
  float: left;
}
.goldimg{
  float: left;
  margin-left: 10rpx;
}
.righttriangle{
  float: right;
  margin-left: 18rpx;
}
.user_wrap{
  position: fixed;
  height: 100%;
  width: 100%;
  background: #1d1a2b;
  z-index: -1
}
